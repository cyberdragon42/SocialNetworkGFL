@model IEnumerable<Domain.Models.Notification>

<h2>Notifications</h2>
@if (Model.Count()==0)
{
    <p>No notifications yet</p>
}
else
{
    @foreach (var item in Model)
     {
            <div>
                <h3>
                    @item.Content
                </h3>
                <p>
                    @item.Date
                </p>
            </div>
     }
}

