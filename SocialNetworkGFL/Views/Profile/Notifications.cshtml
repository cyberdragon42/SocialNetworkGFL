@model IEnumerable<Domain.Models.Notification>

@if (Model.Count()==0)
{
    <p></p>
    <p>No notifications yet</p>
}
else
{
    @foreach (var item in Model)
     {
            <div>
                <h4>
                    @item.Content
                </h4>
                <p>
                    @item.Date
                </p>
                <hr />
            </div>
     }
}


